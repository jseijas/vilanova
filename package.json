{
  "name": "vilanova",
  "description": "JSON stringify/parse pluggable library with support for BigInt",
  "version": "0.1.0",
  "main": "lib/index.js",
  "keywords": [
    "JSON",
    "stringify",
    "parse",
    "bigint",
    "replacer",
    "reviver"
